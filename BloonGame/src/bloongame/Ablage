BloonGame
--
+ main() : void


-- Fenster:JFrame
--
- btnstart : JButton()
- btnreset : JButton()
--
+Fenster()


Statistik : JPanel
--
--
+Statistik()
+refresh(time:time, leben:int, blooncount:int) : void


Game : JPanel
--
-timer: Timer
-statistik : Statistik()
-bloonliste : ArrayList
-highscore: int
-zeit: int
-leben: int
-blooncount: int
--
+ Game()
+gameloop(): void
+ bloondelete() : void
+ bloonbewegen() : void

